generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PaymentHistory {
  id                   String           @id @default(uuid()) @db.Uuid
  amount               String
  method               String
  status               String
  transaction_type     String
  created_at           DateTime         @default(now())
  account_id           String           @db.Uuid
}
